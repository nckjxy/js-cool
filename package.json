{
  "name": "js-cool",
  "version": "1.0.0",
  "description": "一些常用的JS方法",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "scripts": {
    "test": "webpack --config webpack.dev.conf.js",
    "build": "webpack --config webpack.prod.conf.js",
    "es5": "npx babel src -d lib",
    "docs": "jsdoc2md src/**/*.js > API.md",
    "lib": "rm -rf src/**/tempCodeRunnerFile.js && rm -rf ./lib && mkdir -p lib && cp -rf ./src/* lib",
    "lib1": "rm -rf src/**/tempCodeRunnerFile.js && mkdir -p lib && cp -rf ./src/* lib && rm -rf lib/**/*.js && npx babel src -d lib",
    "pub": "git add . && git commit -m $npm_package_version && git pull && git push"
  },
  "dependencies": {
    "core-js": "3.6.4"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.8.7",
    "@saqqdy/prettier-config": "1.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "webpack-merge": "4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saqqdy/js-cool.git"
  },
  "keywords": [
    "js-cool",
    "saqqdy"
  ],
  "author": "saqqdy@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saqqdy/js-cool/issues"
  },
  "homepage": "https://github.com/saqqdy/js-cool#readme"
}
