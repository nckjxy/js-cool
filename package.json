{
    "name": "js-cool",
    "version": "2.3.2",
    "description": "这是一个纯原生ES6开发的Javascript常用方法库",
    "main": "lib/index.js",
    "module": "lib/index.esm.js",
    "browser": {
        "lib/index.js": "lib/index.umd.js"
    },
    "unpkg": "lib/index.js",
    "typings": "lib/index.d.ts",
    "directories": {
        "es": "es",
        "lib": "lib",
        "src": "src"
    },
    "files": [
        "es",
        "lib",
        "typings"
    ],
    "scripts": {
        "gen:version": "tscjs scripts/version",
        "tag:version": "tscjs scripts/tag",
        "build": "rimraf lib es && rollup -c",
        "build:umd": "webpack --config webpack.config.js",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node scripts/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup -c -w",
        "test": "node test/test.js",
        "pretest": "npm run build",
        "dist": "run-s eslint prettier gen:version build build:umd build:types docs prettier:docs",
        "docs": "rimraf docs && typedoc",
        "eslint": "eslint --fix --ext .ts,.js",
        "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "prettier:docs": "prettier --write \"docs/**/*.{json,md}\"",
        "pub": "git add . && git commit -m $npm_package_version && git pull && git push"
    },
    "dependencies": {
        "core-js": "^3.21.1",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@microsoft/api-extractor": "^7.19.4",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@saqqdy/prettier-config": "^1.1.1",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "babel-loader": "^8.2.3",
        "eslint": "^8.11.0",
        "eslint-config-sets": "^1.7.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.31.2",
        "terser-webpack-plugin": "^5.3.1",
        "tsnd": "^1.0.2",
        "typedoc": "^0.22.13",
        "typedoc-plugin-markdown": "^3.11.14",
        "typescript": "^4.6.2",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "peerDependencies": {
        "core-js": ">=3.0.0",
        "tslib": ">=2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/js-cool.git"
    },
    "keywords": [
        "js-cool",
        "es6",
        "javascript",
        "js library",
        "js lib",
        "js tools",
        "saqqdy"
    ],
    "author": "saqqdy<https://github.com/saqqdy>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/js-cool/issues"
    },
    "homepage": "https://github.com/saqqdy/js-cool#readme"
}
